type Query {
    authors: [Author]
    authorById(id: ID!): Author
    person(id:Int!): Person
    getPaper(id:String!):[Paper]
}

type Author {
    id: ID!
    name: String!
    bookList: [Book]
}

type Book {
    id: ID!
    title: String!
    publisher: String
}

type Paper{
    id: String!
    name: String!
}